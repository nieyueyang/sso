<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
 
<mapper namespace="com.user">

	<resultMap id="UserResult" type="com.deyun.sso.pojo.User" >
		<result column="ID" property="id" jdbcType="DECIMAL" />
		<result column="USER_CODE" property="userCode"/>
		<result column="USER_NAME" property="userName" jdbcType="VARCHAR" />
		<result column="JOB_NUMBER" property="jobNumber" jdbcType="VARCHAR" />
		<result column="BANK_NUM" property="bankNum" jdbcType="CHAR" />
		<result column="QQ" property="qq" jdbcType="DECIMAL" />
		<result column="MONEY1" property="money1" jdbcType="DECIMAL" />
		<result column="MONEY2" property="money2" jdbcType="DECIMAL" />
		<result column="CREATE_TIME" property="createTime" jdbcType="TIMESTAMP" />
		<result column="CREATE_TIME2" property="createTime2" jdbcType="DATE" />
	</resultMap>
   <!--<resultMap id="UserExtendResult2" type="com.deyun.sso.pojo.User" extends="UserResult">-->
			<!--<result column="PASSWORD" property="password" jdbcType="VARCHAR" />-->
  <!--</resultMap>-->
    
  <sql id="selectAllItems">
  	ID, USER_CODE, USER_NAME, JOB_NUMBER, BANK_NUM, QQ,CREDIT_AMOUNT,MONEY1,MONEY2,
  	CREATE_TIME2,CREATE_TIME3
  </sql>

  <select id="selectUser" resultMap="UserResult">
     SELECT <include refid="selectAllItems"/>
      FROM base_user

      <include refid="user_getPage_sql"/>
      ORDER BY CREATE_TIME DESC
  </select>

	<sql id="user_getPage_sql">
		<where>
			<if test="id and id != ''">
				and id = #{id}
			</if>
			<if test="userCode and userCode != ''">
				and user_code = #{userCode}
			</if>
			<!--<if test="type and type != ''">-->
				<!--and  bitor(type,#{type})=type-->
			<!--</if>-->

		</where>
	</sql>

  



  


	
	



      
</mapper>