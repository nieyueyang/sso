<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
 
<mapper namespace="com.appFuncGroup">

	<resultMap id="appFuncGroupResult" type="com.deyun.sso.pojo.AppFuncGroup">
		<result column="id" property="id"/>
		<result column="group_code" property="groupCode"/>
		<result column="group_name" property="groupName"/>
		<result column="type" property="type"/>
		<result column="is_delete" property="isDelete"/>
		<result column="remark" property="remark"/>
		<result column="create_account" property="createAccount"/>
		<result column="create_name" property="createName"/>
		<result column="create_date" property="createDate"/>
		<result column="modify_account" property="modifyAccount"/>
		<result column="modify_date" property="modifyDate"/>
	</resultMap>

	<sql id="selectAllItems">
		id, group_code,group_name,type,is_delete,remark,create_account,create_name,
		create_date,modify_account,modify_date
	</sql>

	<select id="queryForList" resultMap="appFuncGroupResult">
		select <include refid="selectAllItems"/>
		from app_func_group
		<include refid="where_sql"/>
	</select>

	<sql id="where_sql">
		<where>
			<if test="id !=null and id != ''">
				and id = #{id}
			</if>
			<if test="groupCode !=null and groupCode != ''">
				and group_code  = #{groupCode}
			</if>
			<if test="groupName !=null and groupName != ''">
				and group_name like concat('%', #{groupName}, '%')
			</if>

		</where>
	</sql>

  



  


	
	



      
</mapper>