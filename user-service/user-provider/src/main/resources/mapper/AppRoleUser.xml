<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
 
<mapper namespace="com.appRoleUser">

	<resultMap id="appRoleResult" type="com.deyun.user.dto.AppRole">
		<result column="id" property="id"/>
		<result column="role_id" property="roleId"/>
		<result column="role_code" property="roleCode"/>
		<result column="user_id" property="userId"/>
		<result column="user_account" property="userAccount"/>
		<result column="sys_id" property="sysId"/>
		<result column="tenant_id" property="tenantId"/>
	</resultMap>

	<sql id="selectAllItems">
		id,role_id,role_code,user_id,user_account,sys_id,tenant_id
	</sql>

	<select id="selectForList" resultMap="appRoleResult">
		select <include refid="selectAllItems"/>
		from app_role
		<include refid="role_getPage_sql"/>
	</select>

	<sql id="role_getPage_sql">
		<where>
			<if test="id and id != ''">
				and id = #{id}
			</if>
			<if test="roleCode and roleCode != ''">
				and role_code = #{roleCode}
			</if>
			<if test="roleName and roleName != ''">
				and role_name = #{roleName}
			</if>
			<if test="roleType and roleType != ''">
				and role_type = #{roleType}
			</if>
			<if test="isactive and isactive != ''">
				and isactive = #{isactive}
			</if>
			<if test="sysId and sysId != ''">
				and sys_id = #{sysId}
			</if>
			<if test="tenantId and tenantId != ''">
				and tenant_id = #{tenantId}
			</if>
			<!--<if test="type and type != ''">-->
				<!--and  bitor(type,#{type})=type-->
			<!--</if>-->

		</where>
	</sql>

  



  


	
	



      
</mapper>