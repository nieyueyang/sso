<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
 
<mapper namespace="com.approle">

	<resultMap id="appRoleResult" type="com.deyun.user.dto.AppRole">
		<result column="id" property="id"/>
		<result column="role_code" property="roleCode"/>
		<result column="role_name" property="roleName"/>
		<result column="role_type" property="roleType"/>
		<result column="isactive" property="isactive"/>
		<result column="sys_id" property="sysId"/>
		<result column="tenant_id" property="tenantId"/>
		<result column="remark" property="remark"/>
		<result column="create_account" property="createAccount"/>
		<result column="create_name" property="createName"/>
		<result column="create_date" property="createDate"/>
		<result column="modify_account" property="modifyAccount"/>
		<result column="modify_date" property="modifyDate"/>
	</resultMap>

	<sql id="selectAllItems">
		id, role_code,role_name,role_type,isactive,sys_id,tenant_id,remark,create_account,create_name,
		create_date,modify_account,modify_date
	</sql>

	<select id="selectAppRole" resultMap="appRoleResult">
		select <include refid="selectAllItems"/>
		from app_user
		<include refid="role_getPage_sql"/>
	</select>

	<sql id="role_getPage_sql">
		<where>
			<if test="id and id != ''">
				and id = #{id}
			</if>
			<if test="role_code and role_code != ''">
				and role_code = #{role_code}
			</if>
			<if test="role_name and role_name != ''">
				and role_name = #{role_name}
			</if>
			<if test="role_type and role_type != ''">
				and role_type = #{role_type}
			</if>
			<if test="isactive and isactive != ''">
				and isactive = #{isactive}
			</if>
			<if test="sys_id and sys_id != ''">
				and sys_id = #{sys_id}
			</if>
			<if test="tenant_id and tenant_id != ''">
				and tenant_id = #{tenant_id}
			</if>
			<!--<if test="type and type != ''">-->
				<!--and  bitor(type,#{type})=type-->
			<!--</if>-->

		</where>
	</sql>

  



  


	
	



      
</mapper>